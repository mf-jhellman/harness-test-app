variable "AWS Account" {
    type = "AmazonWebServicesAccount"

    value "eiger-dev" {}
}

variable "_Environment" {
    value "dev" {
        environment = ["development"]
    }

    value "staging" {
        description = ""
        environment = ["staging"]
    }

    value "production" {
        description = ""
        environment = ["production"]
    }
}

variable "AWS Region" {
    value "us-east-1" {}
}